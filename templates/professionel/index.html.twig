{% extends 'base.html.twig' %}

{% block title %}Professionel index{% endblock %}

{% block body %}
    <h1>Professionel index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Verifier</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for professionel in professionels %}
            <tr>
                <td>{{ professionel.id }}</td>
                <td>{{ professionel.verifier ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('professionel_show', {'id': professionel.id}) }}">show</a>
                    <a href="{{ path('professionel_edit', {'id': professionel.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('professionel_new') }}">Create new</a>
{% endblock %}
